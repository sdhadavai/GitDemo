<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="Suite">

	<groups>
		<run>
			<include name="Purchage" />
		</run>
	</groups>
	<listeners>
		<listener
			class-name="rahulshettyacademy.testcomponents.Listeners" />
	</listeners>
	<test thread-count="5" name="Submit Order Test">
		<classes>
			<class name="rahulshettyacademy.test.SubmitOrderTest" />
		</classes>
	</test> <!-- Test -->
	<test thread-count="5" name="Error Validations Test">
		<classes>
			<class name="rahulshettyacademy.test.ErrorValidationsTest" />
		</classes>
	</test>
</suite> <!-- Suite -->
